{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@spartan-ng/ui-switch-helm": [
        "./ui-switch-helm/src/index.ts"
      ],
      "@spartan-ng/ui-button-helm": [
        "./ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": [
        "./global/ui/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/ui-alert-helm": [
        "./src/app/ui/ui-alert-helm/src/index.ts"
      ],
      "@spartan-ng/ui-alertdialog-helm": [
        "./src/app/ui/ui-alertdialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-badge-helm": [
        "./src/app/ui/ui-badge-helm/src/index.ts"
      ],
      "@spartan-ng/ui-card-helm": [
        "./src/app/ui/ui-card-helm/src/index.ts"
      ],
      "@spartan-ng/ui-carousel-helm": [
        "./src/app/ui/ui-carousel-helm/src/index.ts"
      ],
      "@spartan-ng/ui-dialog-helm": [
        "./src/app/ui/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-formfield-helm": [
        "./src/app/ui/ui-formfield-helm/src/index.ts"
      ],
      "@spartan-ng/ui-input-helm": [
        "./src/app/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/ui-label-helm": [
        "./src/app/ui/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/ui-menu-helm": [
        "./src/app/ui/ui-menu-helm/src/index.ts"
      ],
      "@spartan-ng/ui-select-helm": [
        "./src/app/ui/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/ui-toggle-helm": [
        "./src/app/ui/ui-toggle-helm/src/index.ts"
      ],
      "@spartan-ng/ui-typography-helm": [
        "./src/app/ui/ui-typography-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
